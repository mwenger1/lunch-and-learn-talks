js/reveal.js:2696:					verticalSlidesLength = verticalSlides.length;
js/reveal.js:2698:				// Determine how far away this slide is from the present
js/reveal.js:2699:				distanceX = Math.abs( ( indexh || 0 ) - x ) || 0;
js/reveal.js:2701:				// If the presentation is looped, distance should measure
js/reveal.js:2702:				// 1 between the first and last slides
js/reveal.js:2703:				if( config.loop ) {
