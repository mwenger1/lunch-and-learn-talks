test/qunit-1.12.0.js:38:	 * Provides a normalized error string, correcting an issue
test/qunit-1.12.0.js:39:	 * with IE 7 (and prior) where Error.prototype.toString is
test/qunit-1.12.0.js:40:	 * not properly implemented
test/qunit-1.12.0.js:42:	 * Based on http://es5.github.com/#x15.11.4.4
test/qunit-1.12.0.js:44:	 * @param {String|Error} error
test/qunit-1.12.0.js:45:	 * @return {String} error message
test/qunit-1.12.0.js:47:	errorString = function( error ) {
test/qunit-1.12.0.js:48:		var name, message,
test/qunit-1.12.0.js:49:			errorString = error.toString();
test/qunit-1.12.0.js:50:		if ( errorString.substring( 0, 7 ) === "[object" ) {
test/qunit-1.12.0.js:51:			name = error.name ? error.name.toString() : "Error";
