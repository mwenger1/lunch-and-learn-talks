plugin/markdown/markdown.js:124:	 * Parses a data string into multiple slides based
plugin/markdown/markdown.js:125:	 * on the passed in separator arguments.
plugin/markdown/markdown.js:127:	function slidify( markdown, options ) {
plugin/markdown/markdown.js:129:		options = getSlidifyOptions( options );
plugin/markdown/markdown.js:131:		var separatorRegex = new RegExp( options.separator + ( options.verticalSeparator ? '|' + options.verticalSeparator : '' ), 'mg' ),
plugin/markdown/markdown.js:132:			horizontalSeparatorRegex = new RegExp( options.separator );
plugin/markdown/markdown.js:134:		var matches,
plugin/markdown/markdown.js:135:			lastIndex = 0,
plugin/markdown/markdown.js:136:			isHorizontal,
plugin/markdown/markdown.js:137:			wasHorizontal = true,
plugin/markdown/markdown.js:138:			content,
plugin/markdown/markdown.js:139:			sectionStack = [];
plugin/markdown/markdown.js:141:		// iterate until all blocks between separators are stacked up
plugin/markdown/markdown.js:142:		while( matches = separatorRegex.exec( markdown ) ) {
plugin/markdown/markdown.js:143:			notes = null;
plugin/markdown/markdown.js:145:			// determine direction (horizontal by default)
plugin/markdown/markdown.js:146:			isHorizontal = horizontalSeparatorRegex.test( matches[0] );
plugin/markdown/markdown.js:148:			if( !isHorizontal && wasHorizontal ) {
plugin/markdown/markdown.js:149:				// create vertical stack
plugin/markdown/markdown.js:150:				sectionStack.push( [] );
plugin/markdown/markdown.js:153:			// pluck slide content from markdown input
plugin/markdown/markdown.js:154:			content = markdown.substring( lastIndex, matches.index );
plugin/markdown/markdown.js:156:			if( isHorizontal && wasHorizontal ) {
plugin/markdown/markdown.js:157:				// add to horizontal stack
plugin/markdown/markdown.js:158:				sectionStack.push( content );
plugin/markdown/markdown.js:160:			else {
plugin/markdown/markdown.js:161:				// add to vertical stack
plugin/markdown/markdown.js:162:				sectionStack[sectionStack.length-1].push( content );
plugin/markdown/markdown.js:165:			lastIndex = separatorRegex.lastIndex;
plugin/markdown/markdown.js:166:			wasHorizontal = isHorizontal;
plugin/markdown/markdown.js:169:		// add the remaining slide
plugin/markdown/markdown.js:170:		( wasHorizontal ? sectionStack : sectionStack[sectionStack.length-1] ).push( markdown.substring( lastIndex ) );
plugin/markdown/markdown.js:172:		var markdownSections = '';
plugin/markdown/markdown.js:174:		// flatten the hierarchical stack, and insert <section data-markdown> tags
plugin/markdown/markdown.js:175:		for( var i = 0, len = sectionStack.length; i < len; i++ ) {
plugin/markdown/markdown.js:176:			// vertical
plugin/markdown/markdown.js:177:			if( sectionStack[i] instanceof Array ) {
plugin/markdown/markdown.js:178:				markdownSections += '<section '+ options.attributes +'>';
plugin/markdown/markdown.js:180:				sectionStack[i].forEach( function( child ) {
plugin/markdown/markdown.js:181:					markdownSections += '<section data-markdown>' + createMarkdownSlide( child, options ) + '</section>';
plugin/markdown/markdown.js:184:				markdownSections += '</section>';
plugin/markdown/markdown.js:186:			else {
plugin/markdown/markdown.js:187:				markdownSections += '<section '+ options.attributes +' data-markdown>' + createMarkdownSlide( sectionStack[i], options ) + '</section>';
plugin/markdown/markdown.js:191:		return markdownSections;
