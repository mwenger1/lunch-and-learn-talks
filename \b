plugin/notes/notes.html:547:					function _displayTime( hrEl, minEl, secEl, time) {
plugin/notes/notes.html:549:						var sign = Math.sign(time) == -1 ? "-" : "";
plugin/notes/notes.html:550:						time = Math.abs(Math.round(time / 1000));
plugin/notes/notes.html:551:						var seconds = time % 60;
plugin/notes/notes.html:552:						var minutes = Math.floor( time / 60 ) % 60 ;
plugin/notes/notes.html:553:						var hours = Math.floor( time / ( 60 * 60 )) ;
plugin/notes/notes.html:554:						hrEl.innerHTML = sign + zeroPadInteger( hours );
plugin/notes/notes.html:555:						if (hours == 0) {
plugin/notes/notes.html:556:							hrEl.classList.add( 'mute' );
plugin/notes/notes.html:558:						else {
plugin/notes/notes.html:559:							hrEl.classList.remove( 'mute' );
plugin/notes/notes.html:561:						minEl.innerHTML = ':' + zeroPadInteger( minutes );
plugin/notes/notes.html:562:						if (hours == 0 && minutes == 0) {
plugin/notes/notes.html:563:							minEl.classList.add( 'mute' );
plugin/notes/notes.html:565:						else {
plugin/notes/notes.html:566:							minEl.classList.remove( 'mute' );
plugin/notes/notes.html:568:						secEl.innerHTML = ':' + zeroPadInteger( seconds );
plugin/notes/notes.html:571:					function _updateTimer() {
plugin/notes/notes.html:573:						var diff, hours, minutes, seconds,
plugin/notes/notes.html:574:						now = new Date();
plugin/notes/notes.html:576:						diff = now.getTime() - start.getTime();
plugin/notes/notes.html:578:						clockEl.innerHTML = now.toLocaleTimeString( 'en-US', { hour12: true, hour: '2-digit', minute:'2-digit' } );
plugin/notes/notes.html:579:						_displayTime( hoursEl, minutesEl, secondsEl, diff );
plugin/notes/notes.html:580:						if (timings !== null) {
plugin/notes/notes.html:581:							_updatePacing(diff);
plugin/notes/notes.html:586:					function _updatePacing(diff) {
plugin/notes/notes.html:588:						var slideEndTiming = getTimeAllocated(timings) * 1000;
plugin/notes/notes.html:589:						var currentSlide = Reveal.getSlidePastCount();
plugin/notes/notes.html:590:						var currentSlideTiming = timings[currentSlide] * 1000;
plugin/notes/notes.html:591:						var timeLeftCurrentSlide = slideEndTiming - diff;
plugin/notes/notes.html:592:						if (timeLeftCurrentSlide < 0) {
plugin/notes/notes.html:593:							pacingEl.className = 'pacing behind';
plugin/notes/notes.html:595:						else if (timeLeftCurrentSlide < currentSlideTiming) {
plugin/notes/notes.html:596:							pacingEl.className = 'pacing on-track';
plugin/notes/notes.html:598:						else {
plugin/notes/notes.html:599:							pacingEl.className = 'pacing ahead';
plugin/notes/notes.html:601:						_displayTime( pacingHoursEl, pacingMinutesEl, pacingSecondsEl, timeLeftCurrentSlide );
plugin/notes/notes.html:605:					// Update once directly
plugin/notes/notes.html:606:					_updateTimer();
plugin/notes/notes.html:608:					// Then update every second
plugin/notes/notes.html:609:					setInterval( _updateTimer, 1000 );
plugin/notes/notes.html:611:					function _resetTimer() {
plugin/notes/notes.html:613:						if (timings == null) {
plugin/notes/notes.html:614:							start = new Date();
plugin/notes/notes.html:616:						else {
plugin/notes/notes.html:617:							// Reset timer to beginning of current slide
plugin/notes/notes.html:618:							var slideEndTiming = getTimeAllocated(timings) * 1000;
plugin/notes/notes.html:619:							var currentSlide = Reveal.getSlidePastCount();
plugin/notes/notes.html:620:							var currentSlideTiming = timings[currentSlide] * 1000;
plugin/notes/notes.html:621:							var previousSlidesTiming = slideEndTiming - currentSlideTiming;
plugin/notes/notes.html:622:							var now = new Date();
plugin/notes/notes.html:623:							start = new Date(now.getTime() - previousSlidesTiming);
plugin/notes/notes.html:625:						_updateTimer();
