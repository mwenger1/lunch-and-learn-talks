js/reveal.js:3837:	 * Return a sorted fragments list, ordered by an increasing
js/reveal.js:3838:	 * "data-fragment-index" attribute.
js/reveal.js:3840:	 * Fragments will be revealed in the order that they are returned by
js/reveal.js:3841:	 * this function, so you can use the index attributes to control the
js/reveal.js:3842:	 * order of fragment appearance.
js/reveal.js:3844:	 * To maintain a sensible default fragment order, fragments are presumed
js/reveal.js:3845:	 * to be passed in document order. This function adds a "fragment-index"
js/reveal.js:3846:	 * attribute to each node if such an attribute is not already present,
js/reveal.js:3847:	 * and sets that attribute to an integer value which is the position of
js/reveal.js:3848:	 * the fragment within the fragments list.
js/reveal.js:3850:	 * @param {object[]|*} fragments
js/reveal.js:3851:	 * @return {object[]} sorted Sorted array of fragments
js/reveal.js:3853:	function sortFragments( fragments ) {
js/reveal.js:3855:		fragments = toArray( fragments );
js/reveal.js:3857:		var ordered = [],
js/reveal.js:3858:			unordered = [],
js/reveal.js:3859:			sorted = [];
js/reveal.js:3861:		// Group ordered and unordered elements
js/reveal.js:3862:		fragments.forEach( function( fragment, i ) {
js/reveal.js:3863:			if( fragment.hasAttribute( 'data-fragment-index' ) ) {
js/reveal.js:3864:				var index = parseInt( fragment.getAttribute( 'data-fragment-index' ), 10 );
js/reveal.js:3866:				if( !ordered[index] ) {
js/reveal.js:3867:					ordered[index] = [];
js/reveal.js:3870:				ordered[index].push( fragment );
js/reveal.js:3872:			else {
js/reveal.js:3873:				unordered.push( [ fragment ] );
js/reveal.js:3877:		// Append fragments without explicit indices in their
js/reveal.js:3878:		// DOM order
js/reveal.js:3879:		ordered = ordered.concat( unordered );
js/reveal.js:3881:		// Manually count the index up per group to ensure there
js/reveal.js:3882:		// are no gaps
js/reveal.js:3883:		var index = 0;
js/reveal.js:3885:		// Push all fragments in their sorted order to an array,
js/reveal.js:3886:		// this flattens the groups
js/reveal.js:3887:		ordered.forEach( function( group ) {
js/reveal.js:3888:			group.forEach( function( fragment ) {
js/reveal.js:3889:				sorted.push( fragment );
js/reveal.js:3890:				fragment.setAttribute( 'data-fragment-index', index );
js/reveal.js:3893:			index ++;
js/reveal.js:3896:		return sorted;
js/reveal.js:3901:	 * Navigate to the specified slide fragment.
