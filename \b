js/reveal.js:1278:	 * @return {number}
js/reveal.js:1280:	function distanceBetween( a, b ) {
js/reveal.js:1282:		var dx = a.x - b.x,
js/reveal.js:1283:			dy = a.y - b.y;
js/reveal.js:1285:		return Math.sqrt( dx*dx + dy*dy );
