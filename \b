plugin/notes-server/notes.html:551:			 * Limits the frequency at which a function can be called.
plugin/notes-server/notes.html:553:			function debounce( fn, ms ) {
plugin/notes-server/notes.html:555:				var lastTime = 0,
plugin/notes-server/notes.html:556:					timeout;
plugin/notes-server/notes.html:558:				return function() {
plugin/notes-server/notes.html:560:					var args = arguments;
plugin/notes-server/notes.html:561:					var context = this;
plugin/notes-server/notes.html:563:					clearTimeout( timeout );
plugin/notes-server/notes.html:565:					var timeSinceLastCall = Date.now() - lastTime;
plugin/notes-server/notes.html:566:					if( timeSinceLastCall > ms ) {
plugin/notes-server/notes.html:567:						fn.apply( context, args );
plugin/notes-server/notes.html:568:						lastTime = Date.now();
plugin/notes-server/notes.html:570:					else {
plugin/notes-server/notes.html:571:						timeout = setTimeout( function() {
plugin/notes-server/notes.html:572:							fn.apply( context, args );
plugin/notes-server/notes.html:573:							lastTime = Date.now();
plugin/notes-server/notes.html:574:						}, ms - timeSinceLastCall );
plugin/notes-server/notes.html:582:		</script>
