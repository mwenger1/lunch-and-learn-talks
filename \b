plugin/notes/notes.html:704:				function zeroPadInteger( num ) {
plugin/notes/notes.html:706:					var str = '00' + parseInt( num );
plugin/notes/notes.html:707:					return str.substring( str.length - 2 );
plugin/notes/notes.html:712:				 * Limits the frequency at which a function can be called.
plugin/notes/notes.html:714:				function debounce( fn, ms ) {
plugin/notes/notes.html:716:					var lastTime = 0,
plugin/notes/notes.html:717:						timeout;
plugin/notes/notes.html:719:					return function() {
plugin/notes/notes.html:721:						var args = arguments;
plugin/notes/notes.html:722:						var context = this;
plugin/notes/notes.html:724:						clearTimeout( timeout );
plugin/notes/notes.html:726:						var timeSinceLastCall = Date.now() - lastTime;
plugin/notes/notes.html:727:						if( timeSinceLastCall > ms ) {
plugin/notes/notes.html:728:							fn.apply( context, args );
plugin/notes/notes.html:729:							lastTime = Date.now();
plugin/notes/notes.html:731:						else {
plugin/notes/notes.html:732:							timeout = setTimeout( function() {
plugin/notes/notes.html:733:								fn.apply( context, args );
plugin/notes/notes.html:734:								lastTime = Date.now();
plugin/notes/notes.html:735:							}, ms - timeSinceLastCall );
plugin/notes/notes.html:744:		</script>
