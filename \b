js/reveal.js:378:     * Loads the dependencies of reveal.js. Dependencies are
js/reveal.js:379:     * defined via the configuration option 'dependencies'
js/reveal.js:380:     * and will be loaded prior to starting/binding reveal.js.
js/reveal.js:381:     * Some dependencies may have an 'async' flag, if so they
js/reveal.js:382:     * will load after reveal.js has been started up.
js/reveal.js:384:	function load() {
js/reveal.js:386:		var scripts = [],
js/reveal.js:387:			scriptsAsync = [],
js/reveal.js:388:			scriptsToPreload = 0;
js/reveal.js:390:		// Called once synchronous scripts finish loading
js/reveal.js:391:		function proceed() {
js/reveal.js:392:			if( scriptsAsync.length ) {
js/reveal.js:393:				// Load asynchronous scripts
js/reveal.js:394:				head.js.apply( null, scriptsAsync );
js/reveal.js:397:			start();
js/reveal.js:400:		function loadScript( s ) {
js/reveal.js:401:			head.ready( s.src.match( /([\w\d_\-]*)\.?js$|[^\\\/]*$/i )[0], function() {
js/reveal.js:402:				// Extension may contain callback functions
js/reveal.js:403:				if( typeof s.callback === 'function' ) {
js/reveal.js:404:					s.callback.apply( this );
js/reveal.js:407:				if( --scriptsToPreload === 0 ) {
js/reveal.js:408:					proceed();
js/reveal.js:413:		for( var i = 0, len = config.dependencies.length; i < len; i++ ) {
js/reveal.js:414:			var s = config.dependencies[i];
js/reveal.js:416:			// Load if there's no condition or the condition is truthy
js/reveal.js:417:			if( !s.condition || s.condition() ) {
js/reveal.js:418:				if( s.async ) {
js/reveal.js:419:					scriptsAsync.push( s.src );
js/reveal.js:421:				else {
js/reveal.js:422:					scripts.push( s.src );
js/reveal.js:425:				loadScript( s );
js/reveal.js:429:		if( scripts.length ) {
js/reveal.js:430:			scriptsToPreload = scripts.length;
js/reveal.js:432:			// Load synchronous scripts
js/reveal.js:433:			head.js.apply( null, scripts );
js/reveal.js:435:		else {
js/reveal.js:436:			proceed();
