js/reveal.js:2801:					break;
js/reveal.js:2802:				default:
js/reveal.js:2803:					value.push( indexh + 1 );
js/reveal.js:2804:					if( isVerticalSlide() ) value.push( '.', indexv + 1 );
js/reveal.js:2807:			dom.slideNumber.innerHTML = formatSlideNumber( value[0], value[1], value[2] );
