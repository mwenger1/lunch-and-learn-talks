js/reveal.js:856:	 * Creates a background for the given slide.
js/reveal.js:858:	 * @param {HTMLElement} slide
js/reveal.js:859:	 * @param {HTMLElement} container The element that the background
js/reveal.js:860:	 * should be appended to
js/reveal.js:861:	 * @return {HTMLElement} New background div
js/reveal.js:863:	function createBackground( slide, container ) {
js/reveal.js:865:		var data = {
js/reveal.js:866:			background: slide.getAttribute( 'data-background' ),
js/reveal.js:867:			backgroundSize: slide.getAttribute( 'data-background-size' ),
js/reveal.js:868:			backgroundImage: slide.getAttribute( 'data-background-image' ),
js/reveal.js:869:			backgroundVideo: slide.getAttribute( 'data-background-video' ),
js/reveal.js:870:			backgroundIframe: slide.getAttribute( 'data-background-iframe' ),
js/reveal.js:871:			backgroundColor: slide.getAttribute( 'data-background-color' ),
js/reveal.js:872:			backgroundRepeat: slide.getAttribute( 'data-background-repeat' ),
js/reveal.js:873:			backgroundPosition: slide.getAttribute( 'data-background-position' ),
js/reveal.js:874:			backgroundTransition: slide.getAttribute( 'data-background-transition' )
js/reveal.js:877:		var element = document.createElement( 'div' );
js/reveal.js:879:		// Carry over custom classes from the slide to the background
js/reveal.js:880:		element.className = 'slide-background ' + slide.className.replace( /present|past|future/, '' );
js/reveal.js:882:		if( data.background ) {
js/reveal.js:883:			// Auto-wrap image urls in url(...)
js/reveal.js:884:			if( /^(http|file|\/\/)/gi.test( data.background ) || /\.(svg|png|jpg|jpeg|gif|bmp)([?#]|$)/gi.test( data.background ) ) {
js/reveal.js:885:				slide.setAttribute( 'data-background-image', data.background );
js/reveal.js:887:			else {
