js/reveal.js:1347:	 * Find the closest parent that matches the given
js/reveal.js:1348:	 * selector.
js/reveal.js:1350:	 * @param {HTMLElement} target The child element
js/reveal.js:1351:	 * @param {String} selector The CSS selector to match
js/reveal.js:1352:	 * the parents against
js/reveal.js:1354:	 * @return {HTMLElement} The matched parent or null
js/reveal.js:1355:	 * if no matching parent was found
js/reveal.js:1357:	function closestParent( target, selector ) {
js/reveal.js:1359:		var parent = target.parentNode;
js/reveal.js:1361:		while( parent ) {
js/reveal.js:1363:			// There's some overhead doing this each time, we don't
js/reveal.js:1364:			// want to rewrite the element prototype but should still
js/reveal.js:1365:			// be enough to feature detect once at startup...
js/reveal.js:1366:			var matchesMethod = parent.matches || parent.matchesSelector || parent.msMatchesSelector;
js/reveal.js:1368:			// If we find a match, we're all set
js/reveal.js:1369:			if( matchesMethod && matchesMethod.call( parent, selector ) ) {
js/reveal.js:1370:				return parent;
js/reveal.js:1373:			// Keep searching
js/reveal.js:1374:			parent = parent.parentNode;
js/reveal.js:1378:		return null;
js/reveal.js:1383:	 * Converts various color input formats to an {r:0,g:0,b:0} object.
js/reveal.js:1385:	 * @param {string} color The string representation of a color
js/reveal.js:1386:	 * @example
js/reveal.js:1387:	 * colorToRgb('#000');
js/reveal.js:1388:	 * @example
js/reveal.js:1389:	 * colorToRgb('#000000');
js/reveal.js:1390:	 * @example
js/reveal.js:1391:	 * colorToRgb('rgb(0,0,0)');
js/reveal.js:1392:	 * @example
js/reveal.js:1393:	 * colorToRgb('rgba(0,0,0)');
js/reveal.js:1395:	 * @return {{r: number, g: number, b: number, [a]: number}|null}
js/reveal.js:1397:	function colorToRgb( color ) {
js/reveal.js:1399:		var hex3 = color.match( /^#([0-9a-f]{3})$/i );
js/reveal.js:1400:		if( hex3 && hex3[1] ) {
js/reveal.js:1401:			hex3 = hex3[1];
js/reveal.js:1402:			return {
js/reveal.js:1403:				r: parseInt( hex3.charAt( 0 ), 16 ) * 0x11,
js/reveal.js:1404:				g: parseInt( hex3.charAt( 1 ), 16 ) * 0x11,
js/reveal.js:1405:				b: parseInt( hex3.charAt( 2 ), 16 ) * 0x11
js/reveal.js:1409:		var hex6 = color.match( /^#([0-9a-f]{6})$/i );
js/reveal.js:1410:		if( hex6 && hex6[1] ) {
js/reveal.js:1411:			hex6 = hex6[1];
js/reveal.js:1412:			return {
js/reveal.js:1413:				r: parseInt( hex6.substr( 0, 2 ), 16 ),
js/reveal.js:1414:				g: parseInt( hex6.substr( 2, 2 ), 16 ),
